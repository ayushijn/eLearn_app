<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#65EAF1F0"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Introduction to Queue"
        android:textSize="25sp"
        android:textStyle="bold"
        android:gravity="center"
        android:id="@+id/qintroduction"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"
        android:paddingHorizontal="10dp"
        android:paddingVertical="5dp"
        android:background="#15DEF8"
        android:tag="1"
        android:onClick="clickA"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Implementation of Queue"
        android:textSize="25sp"
        android:textStyle="bold"
        android:gravity="center"
        android:minLines="2"
        android:id="@+id/qimplementation"
        android:layout_below="@+id/qintroduction"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"
        android:paddingHorizontal="10dp"
        android:paddingVertical="5dp"
        android:background="#15DEF8"
        android:tag="2"
        android:onClick="clickA"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Priority Queue"
        android:textSize="25sp"
        android:textStyle="bold"
        android:gravity="center"
        android:minLines="2"
        android:id="@+id/priority"
        android:layout_below="@+id/qimplementation"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"
        android:paddingHorizontal="10dp"
        android:paddingVertical="5dp"
        android:background="#15DEF8"
        android:tag="3"
        android:onClick="clickA"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Circular Queue"
        android:textSize="25sp"
        android:textStyle="bold"
        android:gravity="center"
        android:minLines="2"
        android:id="@+id/circular"
        android:layout_below="@+id/priority"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"
        android:paddingHorizontal="10dp"
        android:paddingVertical="5dp"
        android:background="#15DEF8"
        android:tag="4"
        android:onClick="clickA"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Deque"
        android:textSize="25sp"
        android:textStyle="bold"
        android:gravity="center"
        android:minLines="2"
        android:id="@+id/deque"
        android:layout_below="@+id/circular"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="10dp"
        android:paddingHorizontal="10dp"
        android:paddingVertical="5dp"
        android:background="#15DEF8"
        android:tag="5"
        android:onClick="clickA"/>
    </LinearLayout>
    </ScrollView>

</RelativeLayout>
